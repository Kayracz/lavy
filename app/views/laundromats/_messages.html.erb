<div class="message-container">

  <% laundromats.each do |laundromat| %>
  <div class="message">
    <img class="laundry-pic" src="https://thumbs.dreamstime.com/t/washing-machine-work-home-icon-vector-drawing-doodle-style-background-washing-machine-icon-vector-doodle-style-112393272.jpg">
    <div class="message-body">
      <p><strong><%= laundromat.name %></strong></p>
      <span class="fa fa-star checked"></span>
      <span class="fa fa-star checked"></span>
      <span class="fa fa-star checked"></span>
      <span class="fa fa-star"></span>
      <span class="fa fa-star"></span>
      <p></p>
    </div>
    <div class="message-name">
      <h3><strong>Bags/day:</strong> <%= laundromat.bags_per_day %>  </h3>
      <h3><strong>Phone:</strong> <%= laundromat.phone_number %>  </h3>
      <h3><%= laundromat.address[0..15] %>...</h3>
    </div>
    <div class="message-status">
      <p class="price"> <%= humanized_money_with_symbol(laundromat.price) %></p>
      <%= link_to 'Pick me!', new_laundromat_order_path(laundromat), class: "btn btn-primary btn-xs lavy-color" %>
    </div>
  </div>
  <% end %>
</div>
